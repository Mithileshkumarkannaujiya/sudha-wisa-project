        <parameter name="appPackage" value="com.wisa.summit"/>
        <parameter name="appActivity" value="com.wisa.summit.views.activity.SplashScreen"/>
        <parameter name="app" value="E:\\wisa.apk"/>
        <classes>
            <class name="appium.scripts.SetupWizardTest"/>
