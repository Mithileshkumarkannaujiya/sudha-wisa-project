<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="BetaSuite" parallel="tests" thread-count="2" preserve-order="true">
    <listeners>
        <listener class-name="appium.generics.ProjectListeners"/>
        <!--  <listener class-name="appium.generics.RetryListener"/>-->
    </listeners>
    <test verbose="2" name="Wisa Automation">
        <parameter name="AUTOMATION_NAME" value="Appium"/>
